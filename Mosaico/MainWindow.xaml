<Window x:Class="Mosaico.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mosaico"
        mc:Ignorable="d"
        Title="Mosaico" Height="450" Width="800" MinWidth="500" MinHeight="500" Icon="Assets\MosaicoLogo.ico">
    <DockPanel>
        <TextBlock Width="100" Background="Purple" DockPanel.Dock="Left"></TextBlock>
        <TextBlock Height="100" Background="Purple" DockPanel.Dock="Top"></TextBlock>
        <TextBlock Height="100" Background="Green" DockPanel.Dock="Bottom"></TextBlock>
        <TextBlock Width="100" Background="Green" DockPanel.Dock="Right"></TextBlock>
        <TextBlock Width="100" Background="Orange" DockPanel.Dock="left"></TextBlock>
        <TextBlock Height="100" Background="Orange" DockPanel.Dock="Bottom"></TextBlock>
        <TextBlock Height="100" Background="Pink" DockPanel.Dock="Top"></TextBlock>
        <TextBlock Width="100" Background="Pink" DockPanel.Dock="Right"></TextBlock>
        
        
        
        <TextBlock Background="Black" DockPanel.Dock="Right"></TextBlock>
    </DockPanel>
</Window>
